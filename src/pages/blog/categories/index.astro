---
import Main from "../../../layouts/Main.astro";

import { Categories } from "../../../content/config"

const pageName = "Post Categories";
---

<Main {pageName}>
  <p>Categories will be displayed here...</p>
  <ul>
    {Object.values(Categories).map((category: string) => <li><a href={`/blog/categories/${category}`}>{category}</a></li>)}
  </ul>
</Main>../../../constants

<style>
  li {
    list-style-type: circle;
  }
</style>