---
import BaseLayout from "../layouts/BaseLayout.astro";

const title = "About Me";
const description = "Learn more about me and my background.";
---

<BaseLayout title={title} description={description}>
  <h1>{title}</h1>
  <p>
    I am a software engineer with a passion for building web applications. I
    have experience working with JavaScript, React, Node.js, and more. I am
    always looking to learn new technologies and improve my skills.
  </p>
</BaseLayout>
